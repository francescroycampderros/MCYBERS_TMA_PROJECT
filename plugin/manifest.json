{
  "manifest_version": 3,
  "name": "GDPR Compliance Auditor",
  "version": "1.0",
  "description": "Check GDPR compliance score for the current website.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "host_permissions": [
    "http://localhost:3000/*"
  ]
}
